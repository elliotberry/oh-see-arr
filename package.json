{
    "name": "oh-see-arr",
    "description": "Selects a portion of your screen, outputs the text to terminal. Uses Tesseract.js, doesn't send your stuff to the cloud.",
    "categories": [
        "CLI",
        "OCR",
        "Tesseract.js",
        "Mac",
        "Linux",
        "Command Line",
        "Optical Character Recognition"
    ],
    "version": "0.1.2",
    "author": "ɛʟʟɨօȶ b  =^._.^= ∫ <elliot@email.gd>",
    "license": "The Unlicense",
    "main": "index.js",
    "bin": {
        "ocr": "./index.js"
    },
    "type": "module",
    "dependencies": {
        "screencapture": "^0.3.1",
        "tesseract.js": "^5.1.1"
    },
    "devDependencies": {
        "@stylistic/eslint-plugin-js": "^2.10.1",
        "eslint": "^9.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-perfectionist": "^3.9.1",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-unicorn": "^56.0.0",
        "prettier": "^3.3.3",
        "release-it": "^17.10.0"
    },
    "prettier": {
        "arrowParens": "always",
        "bracketSameLine": true,
        "bracketSpacing": true,
        "semi": false,
        "experimentalTernaries": true,
        "singleQuote": false,
        "jsxSingleQuote": false,
        "quoteProps": "as-needed",
        "trailingComma": "none",
        "singleAttributePerLine": false,
        "htmlWhitespaceSensitivity": "css",
        "vueIndentScriptAndStyle": false,
        "proseWrap": "preserve",
        "insertPragma": false,
        "printWidth": 80,
        "requirePragma": false,
        "tabWidth": 2,
        "useTabs": false,
        "embeddedLanguageFormatting": "auto"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    }
}
